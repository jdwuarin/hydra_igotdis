<% if is_public_page %>

  <%= render '/matches/most_first_blood_cell/active_static',
      t_contestant: t_contestant %>

<% else %>

  <th class="firstblood active" colspan="3">
    <%= content_tag :div, t_contestant.id.to_s,
      hidden: true, class: "contestant" %>
    <img class="swordlogo" src= <%= asset_path("sword.png") %> >
    <p>Most First Bloods</p>
  </th>

<% end %>